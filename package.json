{
    "name": "tictactoe",
    "version": "0.1.0",
    "private": true,
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.4.2",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "tsc webpack.config.ts && npm run webpack -- --env mode=production",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "dev": "tsc webpack.config.ts && webpack serve -- --env mode=development --config webpack.config.js",
        "duplicate": "webpack --profile --json > stats.json && npx webpack-stats-duplicates stats.json",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/preset-env": "^7.17.10",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/styled-components": "^5.1.25",
        "babel-loader": "^8.2.5",
        "babel-plugin-styled-components": "^2.0.7",
        "css-loader": "^6.7.1",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.4.1",
        "prettier": "2.6.2",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "styled-components": "^5.3.5",
        "svg-inline-loader": "^0.8.2",
        "svg-react-loader": "^0.4.6",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0",
        "webpack-stats-duplicates": "^2.1.0"
    }
}
